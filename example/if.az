-- Ternary like if expression
fn ternary_like(x: Int): Int = 
  if x == 5 
    then 1
    else 0

extern console_log(s: String)

-- Block if expression, familiar to Javascript programmers
fn block_if(x: Int): Unit = do
  if x == 5 
    then do
      console_log("x is 5")
    else do
      console_log("x is not 5")
  end
end